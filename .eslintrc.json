{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
			"no-console": "off",
			"react/jsx-props-no-spreading": "off",
            "react/jsx-indent": ["error", 4, { "checkAttributes": true, "indentLogicalExpressions": true }],
            "react/jsx-indent-props": ["error", 4],
			"indent": ["error", 4],
			"linebreak-style": ["error", "unix"],
			"quotes": ["error", "double"],
			"semi": ["error", "always"],
			"no-empty": "warn",
			"no-cond-assign": ["error", "always"]
		}
}
